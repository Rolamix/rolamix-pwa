{
  "name": "@stencil/starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production stencil build",
    "buildWPrerender": "NODE_ENV=production stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "devWithSW": "sd concurrent \"stencil build --dev --service-worker --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "start:ssr": "NODE_ENV=production stencil build --ssr && NODE_ENV=production node server.js",
    "start:ssr:dev": "sd concurrent \"NODE_ENV=production stencil build --dev --watch\" \"NODE_ENV=production node server.js\"",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache",
    "lint": "tslint --project ."
  },
  "dependencies": {
    "@stencil/core": "^0.9.7",
    "@theracode/router": "/Users/patricksears/Git/EngineerApart/theracode/open-source/packages/router",
    "express": "^4.16.3",
    "ionicons": "^4.1.2"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@theracode/tslint-rules": "^0.0.1",
    "@types/jest": "^22.2.3",
    "jest": "^23.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-app-starter.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://github.com/ionic-team/stencil",
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
